<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.oldering.sample.ticker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="io.oldering.sample.ticker.TickerActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="io.oldering.sample.ticker.SaveActivity"
            android:parentActivityName="io.oldering.sample.ticker.TickerActivity" />
        <activity
            android:name="io.oldering.sample.ticker.TransparentActivity"
            android:theme="@style/MyTransparentActivity"
            android:parentActivityName="io.oldering.sample.ticker.TickerActivity" />
    </application>

</manifest>